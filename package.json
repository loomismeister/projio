{
  "name": "projio",
  "version": "1.0.0",
  "description": "A lightweight project planner",
  "main": "app/background.js",
  "presets": [
    "es2015",
    "react"
  ],
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "start": "gulp start",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loomismeister/projio.git"
  },
  "author": "Mike Loomis",
  "license": "MIT",
  "build": {
    "appId": "com.projio",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "homepage": "https://github.com/loomismeister/projio#readme",
  "devDependencies": {
    "alt": "^0.18.6",
    "alt-container": "^1.0.2",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "^3.5.0",
    "electron": "^1.4.1",
    "electron-builder": "^10.14.0",
    "electron-react-devtools": "^0.4.0",
    "electron-reload": "^1.0.3",
    "gulp": "^3.9.0",
    "gulp-batch": "^1.0.5",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "rollup": "0.38.1",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "6.0.1",
    "semantic-ui": "^2.2.6"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "fs-jetpack": "^0.10.2",
    "sqlite3": "^3.1.8"
  }
}
